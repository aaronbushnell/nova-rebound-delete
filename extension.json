{
    "identifier": "aaronbushnell.ReboundDelete",
    "name": "Rebound Delete",
    "organization": "Aaron Bushnell",
    "description": "Change Nova's CMD+Backspace to behave like Sublime Text",
    "version": "1.0.2",
    "categories": ["commands"],
    "bugs": "https://github.com/aaronbushnell/nova-rebound-delete/issues",
    
    "main": "main.js",
    
    "activationEvents": [],

    "entitlements": {
        
    },
    
    "commands": {
        "editor": [
            {
                "title": "Delete Line using Rebound",
                "command": "rebound-delete.reboundDelete",
                "when": "editorHasFocus",
                "shortcut": "cmd-backspace"
            }
        ]
    }
}
